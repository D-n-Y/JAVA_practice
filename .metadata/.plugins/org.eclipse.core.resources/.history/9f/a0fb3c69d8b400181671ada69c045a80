package com.sion.test;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		
		Scanner scanner = new Scanner(System.in);
		int num = scanner.nextInt();
		

		
			
		for(int i = 0; i < 10; i++) {
			System.out.println("number : " + num+i);			
			System.out.println("number : " + (num+i));
			
		}
	}
}
